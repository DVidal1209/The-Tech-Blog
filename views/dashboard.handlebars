{{#each blogs as |blog|}}
<div class="container">
    <div class="row">
        <h2>{{{blog.title}}}</h2>
    </div>
    <div class="row">
        <p>
            {{{blog.content}}}
        </p>
    </div>
    <div class="row">
        <div class="col-6">
            <button class="col-6" id="update">Update</button>
        </div>
        <div class="col-6">
            <button class="col-6" id="delete">Delete</button>
        </div>
    </div>
</div>
{{/each}}